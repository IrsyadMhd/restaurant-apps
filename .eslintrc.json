{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  },
  "plugins": ["jasmine"]
}
